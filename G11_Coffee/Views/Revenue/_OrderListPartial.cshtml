@model IEnumerable<G11_Coffee.Models.Order>

<table class="table">
    <thead>
        <tr>
            <th>Order ID</th>
            <th>Order Date</th>
            <th>Employee</th>
            <th>Cafe</th>
            <th>Total Amount</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.Id</td>
                <td>@order.OrderDate.ToShortDateString()</td>
                <td>@order.Employee.FullName</td>
                <td>@order.Cafe.Name</td>
                <td>@order.TotalAmount.ToString("C")</td>
            </tr>
        }
    </tbody>
</table>
